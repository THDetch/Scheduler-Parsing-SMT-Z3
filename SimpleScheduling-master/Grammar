<scheduling_problem> ::= <task_description> | <task_description> <scheduling_problem>
<task_description> ::= <task_name>  takes  <duration>  needs <dependencies>

<task_name> ::= <alpha> <alphanumeric>*
<duration> ::= <non_zero_digit> <digit>*
<dependencies> ::= none | <expressions>

<expression> ::= <term> | <expression> or <term>
<term> ::= <factor> | <term> and <factor
<factor> ::= <task_name> | ( <expression> )


<alphanumeric> ::= <alpha> | <digit>
<alpha> ::= A | B | ... | Z | a | b | ... | z
<non_zero_digit> ::= 1 | 2 | 3 | 4 | 5 | 6 | 7 | 8 | 9
<digit> ::= 0 | <non_zero_digit>




